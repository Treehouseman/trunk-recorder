+	for(int i = 0; i < 10; i++){
+		int minm = 100;
+		for(int x = 0; x < 60; x++){
+			if(syshistory[i][x] < minm){
+				minm = syshistory[i][x];
+			}
+		}
+		minmsg[i]=minm;
+	}
+	for(int i = 0; i < 10; i++){
+		int maxm = 0;
+		for(int x = 0; x < 60; x++){
+			if(syshistory[i][x] > maxm){
+				maxm = syshistory[i][x];
+			}
+		}
+		maxmsg[i]=maxm;
+	}
+	for(int i = 0; i < 10; i++){
+		int avgm = 0;
+		for(int x = 0; x < 60; x++){
+			avgm = avgm + syshistory[i][x];
+		}
+		avgmsg[i]=avgm/60;
+	}
+	}
+	else{
+	for(int i = 0; i < 10; i++){
+		csysid[i] = sysids[i];
+	}
+	for(int i = 0; i < 10; i++){
+		int minm = 100;
+		for(int x = 0; x < pos; x++){
+			if(syshistory[i][x] < minm){
+				minm = syshistory[i][x];
+			}
+		}
+		minmsg[i]=minm;
+	}
+	for(int i = 0; i < 10; i++){
+		int maxm = 0;
+		for(int x = 0; x < pos; x++){
+			if(syshistory[i][x] > maxm){
+				maxm = syshistory[i][x];
+			}
+		}
+		maxmsg[i]=maxm;
+	}
+	for(int i = 0; i < 10; i++){
+		int avgm = 0;
+		for(int x = 0; x < pos; x++){
+			avgm = avgm + syshistory[i][x];
