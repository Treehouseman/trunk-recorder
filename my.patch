+void Tree::CPU(){
+	if(!curseenable)
+		return;
+	for(int i = 0; i < 100; i++){
+		Speed[i] = 0;
+		CPUPer[i]=0;
+	}
+	int cpu = 0;
+	const char* match;
+	std::string temp;
+	std::ifstream file;
+	file.open("/sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_max_freq");
+	//printf("Getting Max\n");
+	while(file.good()){
+		file >> Max;
+		//printf("Max is %ld\n",Max);
+		if(Max != 0){
+			Max = Max/1000;
+			break;
+		}
+	}
+	file.close();
+	file.open("/sys/devices/system/cpu/cpu0/cpufreq/cpuinfo_min_freq");
+	//printf("Getting Min\n");
+	while(file.good()){
+		file >> Min;
+		//printf("Min is %ld\n",Min);
+		if(Min != 0){
+			Min = Min/1000;
+			break;
+		}
+	}
+	file.close();
+	file.open("/proc/cpuinfo");
+	while(std::getline(file, temp)){
+		const char *temp2 = temp.c_str();
+		match = strstr (temp2, "cpu MHz");
+		if(match != NULL){
+			sscanf (match, "cpu MHz : %ld", &Speed[cpu]);
+			cpu++;
+		}
+		temp = "";
+	}
+	file.close();
+	for(int i = 0; i < 100; i++){
+		if(Speed[i]!=0){
+			float sp = Speed[i]-Min;
+			float mx = Max-Min;
+			float per = ((sp/mx)*100);
+			CPUPer[i]=per;
+		}
+	}
+}
